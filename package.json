{
  "name": "valibotx",
  "type": "module",
  "version": "2.2.0",
  "packageManager": "pnpm@9.8.0",
  "description": "Extensions for valibot",
  "author": "Ilya Semenov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IlyaSemenov/valibotx"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "!src/**/*.test.*",
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint --fix .",
    "prepare": "husky",
    "prepublishOnly": "pnpm build",
    "test": "vitest run"
  },
  "peerDependencies": {
    "valibot": ">=0.31.0-rc.10"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.0",
    "@changesets/cli": "^2.26.2",
    "@tsconfig/recommended": "^1.0.3",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "valibot": "^0.32.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  }
}
